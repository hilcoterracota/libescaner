<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageId>LibEscaner</PackageId>
    <Description>Libreria para gestion de escaners</Description>
    <Version>1.0.1</Version>
    <Authors>Edgar Maceda</Authors>
    <Company>HilcoTerracota</Company>
    <PackageProjectUrl>https://github.com/edgarmaceda/libescaner</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="3.1.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="3.1.1" />
  </ItemGroup>

</Project>